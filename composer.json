{
    "name": "nextcloud/roomvox",
    "description": "CalDAV-native room booking for Nextcloud",
    "type": "nextcloud-app",
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "nextcloud/ocp": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "OCA\\RoomVox\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OCA\\RoomVox\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "platform": {
            "php": "8.1"
        }
    }
}
